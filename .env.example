# Example environment configuration for dataset-db

# Ingestion settings
INGEST__HF_USERNAME=nhagar
INGEST__DATASET_NAME_SUFFIX=_urls
INGEST__BATCH_SIZE=10000
INGEST__MAX_WORKERS=4
INGEST__ROW_GROUP_SIZE=134217728
INGEST__COMPRESSION=zstd
INGEST__COMPRESSION_LEVEL=6

# Storage settings
STORAGE__STORAGE_TYPE=local
STORAGE__BASE_PATH=./data
# STORAGE__S3_BUCKET=my-bucket
# STORAGE__S3_REGION=us-east-1
STORAGE__DOMAIN_PREFIX_CHARS=2

# Index settings
INDEX__POSTINGS_SHARDS=1024
INDEX__MPHF_GAMMA=2.0

# Global settings
LOG_LEVEL=INFO
